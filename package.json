{
  "name": "@wmfs/cardscript-to-quasar",
  "description": "Produces HTML with Quasar-markup from Cardscript JSON",
  "version": "0.0.0-semantically-released",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "dottie": "2.0.1",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.6.1",
    "conventional-changelog-metahub": "3.0.0",
    "cz-conventional-changelog": "3.0.2",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "semantic-release": "15.13.30",
    "standard": "12.0.1",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/git": "7.0.18",
    "@wmfs/cardscript-examples": "1.16.0"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-to-quasar.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-to-quasar/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "ignore": [
      "lib/utils/local-util.js"
    ]
  }
}
